      <Table name="dimaddress" alias="">
      </Table>
      <Level name="City" visible="true" column="City" type="String" uniqueMembers="false" levelType="Regular">
      </Level>
      <Level name="State" visible="true" column="StateProvince" type="String" uniqueMembers="false" levelType="Regular">
      </Level>
      <Level name="Country" visible="true" column="CountryRegion" type="String" uniqueMembers="false" levelType="Regular">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" visible="true" name="Vendor">
    <Hierarchy name="dimvendor" visible="true" hasAll="true" allMemberName="All Vendor" primaryKey="VendorID">
