    <DimensionUsage source="Product" name="product" visible="true" foreignKey="FK_ProductID">
    </DimensionUsage>
    <DimensionUsage source="Vendor" name="vendor" visible="true" foreignKey="FK_VendorID">
    </DimensionUsage>
    <DimensionUsage source="Ship Method" name="ship method" visible="true" foreignKey="FK_ShipMethodID">
    </DimensionUsage>
    <Measure name="Total Due" column="TotalDue" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Order Quantity" column="OrderQty" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Received Quantity" column="ReceivedQty" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Rejected Quantity" column="RejectedQty" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Stocked Quantity" column="StockedQty" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Line Total" column="LineTotal" aggregator="sum" visible="true">
    </Measure>
  </Cube>
  <Cube name="ProductionCube" visible="true" cache="true" enabled="true">
    <Table name="factproduction" alias="">
    </Table>
    <DimensionUsage source="Product" name="product" visible="true" foreignKey="FK_ProductID">
    </DimensionUsage>
    <DimensionUsage source="Location" name="location" visible="true" foreignKey="FK_LocationID">
    </DimensionUsage>
    <DimensionUsage source="Date" name="start date" visible="true" foreignKey="FK_StartDateID">
