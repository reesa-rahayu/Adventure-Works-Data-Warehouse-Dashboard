<Schema name="AdventureWorks">
  <Dimension type="TimeDimension" visible="true" name="Date">
    <Hierarchy name="dimdate" visible="true" hasAll="true" allMemberName="All Date" primaryKey="DateID">
      <Table name="dimdate" alias="">
      </Table>
      <Level name="Year" visible="true" table="dimdate" column="YearNumber" uniqueMembers="false" levelType="TimeYears" hideMemberIf="Never">
      </Level>
      <Level name="Quarter" visible="true" column="QuarterNumber" type="Numeric" uniqueMembers="false" levelType="TimeQuarters" hideMemberIf="Never">
      </Level>
      <Level name="Month" visible="true" column="MonthName" type="String" uniqueMembers="false" levelType="TimeMonths" hideMemberIf="Never">
      </Level>
      <Level name="Week" visible="true" column="WeekNumber" type="Numeric" uniqueMembers="false" levelType="TimeWeeks" hideMemberIf="Never">
      </Level>
      <Level name="Day" visible="true" column="DayNumber" type="Numeric" uniqueMembers="false" levelType="TimeDays" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" visible="true" name="Customer">
    <Hierarchy name="dimcustomer" visible="true" hasAll="true" allMemberName="All Customer" primaryKey="CustomerID">
      <Table name="dimcustomer" alias="">
      </Table>
      <Level name="Country" visible="true" column="Country" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Territory" visible="true" column="TerritoryName" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Name" visible="true" column="FullName" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" visible="true" name="Product">
    <Hierarchy name="dimproduct" visible="true" hasAll="true" allMemberName="All Product" primaryKey="ProductID">
      <Table name="dimproduct" alias="">
      </Table>
      <Level name="Category" visible="true" column="CategoryName" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="SubCategory" visible="true" column="SubcategoryName" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Name" visible="true" column="ProductName" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" visible="true" name="Location">
    <Hierarchy name="dimlocation" visible="true" hasAll="true" allMemberName="All Location" primaryKey="LocationID">
