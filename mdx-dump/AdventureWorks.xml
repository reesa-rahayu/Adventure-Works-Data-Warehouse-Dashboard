  <Cube name="SalesCube" visible="true" cache="true" enabled="true">
    <Table name="factsales" alias="">
    </Table>
    <DimensionUsage source="Date" name="date" visible="true" foreignKey="FK_DateID">
    </DimensionUsage>
    <DimensionUsage source="Product" name="product" visible="true" foreignKey="FK_ProductID">
    </DimensionUsage>
    <DimensionUsage source="Customer" name="customer" visible="true" foreignKey="FK_CustomerID">
    </DimensionUsage>
    <DimensionUsage source="Sales Person" name="salesperson" visible="true" foreignKey="FK_SalesPersonID">
    </DimensionUsage>
    <DimensionUsage source="Address" name=" ship address" visible="true" foreignKey="FK_ShipToAddressID">
    </DimensionUsage>
    <DimensionUsage source="Address" name="bill address" visible="true" foreignKey="FK_BillToAddressID">
    </DimensionUsage>
    <DimensionUsage source="Special Offer" name="special offer" visible="true" foreignKey="FK_SpecialOfferID">
    </DimensionUsage>
    <DimensionUsage source="Ship Method" name="ship method" visible="true" foreignKey="FK_ShipMethodID">
    </DimensionUsage>
    <Measure name="Sales Amount" column="LineTotal" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Total Due" column="TotalDue" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Order Quantity" column="OrderQty" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Tax Amt" column="TaxAmt" datatype="Integer" aggregator="distinct-count" visible="true">
    </Measure>
    <Measure name="Freight" column="Freight" aggregator="distinct-count" visible="true">
