  <Dimension type="StandardDimension" visible="true" name="Scrap Reason">
    <Hierarchy name="dimscrapreason" visible="true" hasAll="true" allMemberName="All ScrapReason" primaryKey="ScrapReasonID">
      <Table name="dimscrapreason" alias="">
